{
  "name": "helpful-information",
  "homepage": "https://helpfulinformation.redcross.nl/",
  "scripts": {
    "ionic": "ionic",
    "ng": "ng",
    "prepare": "husky install",
    "start": "ng serve --port 0",
    "set-env-variables": "node ./_set-env-variabes.js",
    "prebuild": "npm run set-env-variables",
    "build": "ng build",
    "build:production": "npm run build -- --prod",
    "preserve:production": "cp www/index.html www/404.html",
    "serve:production": "npx http-server ./www/ -c-1",
    "start:production": "npm run build:production && npm run serve:production",
    "test": "ng test --no-watch --browsers=ChromeHeadless",
    "test:dev": "ng test",
    "lint": "npm run lint:syntax",
    "lint:syntax": "prettier --check \"**/*.{md,js,ts,scss,html}\"",
    "fix:syntax": "npm run lint:syntax -- --write",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/forms": "^7.2.16",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/pwa": "^0.13.10",
    "@angular/router": "^7.2.16",
    "@angular/service-worker": "^7.2.16",
    "@auth0/angular-jwt": "^4.1.2",
    "@ionic/angular": "^4.11.8",
    "@ionic/cli": "^6.11.8",
    "@microsoft/applicationinsights-web": "^2.5.9",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "core-js": "2.6.8",
    "rxjs": "^6.6.2",
    "tslib": "^1.12.0",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.13.10",
    "@angular-devkit/build-angular": "^0.13.10",
    "@angular-devkit/core": "^7.3.10",
    "@angular-devkit/schematics": "^7.3.10",
    "@angular/cli": "^7.3.10",
    "@angular/compiler": "^7.2.16",
    "@angular/compiler-cli": "^7.2.16",
    "@angular/language-service": "^7.2.16",
    "@ionic/angular-toolkit": "^1.5.1",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "10.x",
    "codelyzer": "^5.2.2",
    "husky": "^7.0.4",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "^12.1.2",
    "node-sass": "^4.14.1",
    "prettier": "2.2.0",
    "prettier-plugin-organize-imports": "^1.1.1",
    "protractor": "^7.0.0",
    "ts-node": "~9.1.0",
    "tslint": "~5.20.1",
    "typescript": "~3.2.4"
  },
  "lint-staged": {
    "*.{md,js,ts,scss,html}": "prettier --write"
  }
}
